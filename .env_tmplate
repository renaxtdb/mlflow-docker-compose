USER_NAME = <user name>
COMPOSE_PROJECT_NAME=mlflow_${USER_NAME}

# Mount volume path
MOUNT_PATH = <Repository path>
# port setting
JUPYTER_PORT_NO = 8819

# docker image version
PYTHON_VERSION = 3.7
DEBIAN_VERSION = slim-buster

# postgresql config
HOST=mlflow.dev
POSTGRES_USER = <user name>
POSTGRES_PASSWORD = <password>
POSTGRES_POERT=5432
POSTGRES_DB_NAME=mlflow-db
DB_URL = postgresql+psycopg2://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgresql:${POSTGRES_POERT}/${POSTGRES_DB_NAME}
ARTIFACT_PATH = s3://default/

# Minio config
MINIO_ACCESS_KEY=<user name>
MINIO_SECRET_KEY=<password>
MINIO_MOUNT_PATH=<artifacts dir path>
MINIO_PORT=9000

#container resources
CONTAINER_LIMIT_MEMORY = 8g
CONTAINER_USE_CPU = 2


##### Automatically generated environment parameter
MINIO_URL = http://minio:${MINIO_PORT}
